{
    "manifest_version": 2,
    "name": "Quran Word Insights",
    "version": "0.9.0",
    "version_name": "beta",
    "description": "Enhance your Quran.com experience with a deeper understanding of Quranic words",
    "icons": {
      "48": "icons/alm48.png",
      "128": "icons/alm128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.quran.com/*", "*://quran.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
}
